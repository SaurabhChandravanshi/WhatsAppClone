.leftHeader
    position: relative;
    z-index: 1000;
    box-sizing: border-box;
    display: flex;
    flex: none;
    align-items: center;
    justify-content: flex-end;
    width: 100%;
    background-color: #f0f2f5;
    height: 59px;
.line
    display: flex;
    flex: 0 0 auto;
    flex-direction: column;
.headerItems
    display: flex;
    flex-direction: row;
    width: 100%;
    margin: 10px 20px;
    justify-content: space-between;
.profile
    display: flex;
    width: 40px;
    cursor pointer;
    height: 40px;
.menus
    display: flex;
    flex-direction: row;
    column-gap: 20px;
    margin: 10px 0px;
.chatIcon
    cursor :pointer;
.statusIcon
    cursor: pointer;
.menuIcon
    cursor:pointer;

